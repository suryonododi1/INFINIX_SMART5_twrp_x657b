# recovery.fstab for Infinix Smart 5 (X657B)
# Mount points for TWRP

/dev/block/platform/bootdevice/by-name/system      /system     ext4    ro,barrier=1,discard     wait,slotselect
/dev/block/platform/bootdevice/by-name/vendor      /vendor     ext4    ro,barrier=1,discard     wait,slotselect
/dev/block/platform/bootdevice/by-name/product     /product    ext4    ro,barrier=1,discard     wait,slotselect
/dev/block/platform/bootdevice/by-name/cache       /cache      ext4    nosuid,nodev,noatime     wait,check,formattable
/dev/block/platform/bootdevice/by-name/userdata    /data       ext4    nosuid,nodev,noatime     wait,check,encryptable=footer,quota,formattable,resize
/dev/block/platform/bootdevice/by-name/misc        /misc       emmc    defaults                 defaults
/dev/block/platform/bootdevice/by-name/recovery    /recovery   emmc    defaults                 defaults
/dev/block/platform/bootdevice/by-name/boot        /boot       emmc    defaults                 defaults
/dev/block/platform/bootdevice/by-name/vbmeta      /vbmeta     emmc    defaults                 defaults
/dev/block/platform/bootdevice/by-name/metadata    /metadata   ext4    nosuid,nodev,noatime     wait,check,formattable
/dev/block/platform/bootdevice/by-name/frp         /frp        emmc    defaults                 defaults
/dev/block/platform/bootdevice/by-name/persist     /persist    ext4    nosuid,nodev,noatime     wait,check,formattable
/dev/block/platform/bootdevice/by-name/cache       /sdcard     vfat    defaults                 voldmanaged=sdcard:auto,encryptable=userdata
